@model CatalogSettingsModel

<div class="card-body">
    <div class="form-group row">
        <div class="col-md-3">
            <tvprog-override-store-checkbox asp-for="TvChannelUrlStructureTypeId_OverrideForStore" asp-input="TvChannelUrlStructureTypeId" asp-store-scope="@Model.ActiveStoreScopeConfiguration" />
            <tvprog-label asp-for="TvChannelUrlStructureTypeId" />
        </div>
        <div class="col-md-9">
            <tvprog-select asp-for="TvChannelUrlStructureTypeId" asp-items="Model.TvChannelUrlStructureTypes" />
            <span asp-validation-for="TvChannelUrlStructureTypeId"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <tvprog-override-store-checkbox asp-for="ShowFreeShippingNotification_OverrideForStore" asp-input="ShowFreeShippingNotification" asp-store-scope="@Model.ActiveStoreScopeConfiguration" />
            <tvprog-label asp-for="ShowFreeShippingNotification" />
        </div>
        <div class="col-md-9">
            <tvprog-editor asp-for="ShowFreeShippingNotification" />
            <span asp-validation-for="ShowFreeShippingNotification"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <tvprog-override-store-checkbox asp-for="AllowViewUnpublishedTvChannelPage_OverrideForStore" asp-input="AllowViewUnpublishedTvChannelPage" asp-store-scope="@Model.ActiveStoreScopeConfiguration" />
            <tvprog-label asp-for="AllowViewUnpublishedTvChannelPage" />
        </div>
        <div class="col-md-9">
            <tvprog-editor asp-for="AllowViewUnpublishedTvChannelPage" />
            <span asp-validation-for="AllowViewUnpublishedTvChannelPage"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <tvprog-override-store-checkbox asp-for="DisplayDiscontinuedMessageForUnpublishedTvChannels_OverrideForStore" asp-input="DisplayDiscontinuedMessageForUnpublishedTvChannels" asp-store-scope="@Model.ActiveStoreScopeConfiguration" />
            <tvprog-label asp-for="DisplayDiscontinuedMessageForUnpublishedTvChannels" />
        </div>
        <div class="col-md-9">
            <tvprog-editor asp-for="DisplayDiscontinuedMessageForUnpublishedTvChannels" />
            <span asp-validation-for="DisplayDiscontinuedMessageForUnpublishedTvChannels"></span>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-3">
            <tvprog-override-store-checkbox asp-for="AttributeValueOutOfStockDisplayType_OverrideForStore" asp-input="AttributeValueOutOfStockDisplayType" asp-store-scope="@Model.ActiveStoreScopeConfiguration" />
            <tvprog-label asp-for="AttributeValueOutOfStockDisplayType" />
        </div>
        <div class="col-md-9">
            <tvprog-select asp-for="AttributeValueOutOfStockDisplayType" asp-items="Model.AttributeValueOutOfStockDisplayTypes" />
            <span asp-validation-for="AttributeValueOutOfStockDisplayType"></span>
        </div>
    </div>
</div>