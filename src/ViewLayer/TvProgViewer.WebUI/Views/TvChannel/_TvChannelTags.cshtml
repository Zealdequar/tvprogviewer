@model IList<TvChannelTagModel>

@using TvProgViewer.Core.Domain.Catalog

@if (Model.Count > 0)
{
    <div class="tvchannel-tags-box">
        <div class="tvchannel-tags-list">
            <ul>
                @for (var i = 0; i < Model.Count; i++)
                {
                    <li>
                       <a href="@(Url.RouteUrl<TvChannelTag>(new { SeName = Model[i].SeName }))" class="tvchannel-tag">
                           @Model[i].Name
                       </a> 
                    </li>
                    
                }
            </ul>
        </div>
    </div>
}