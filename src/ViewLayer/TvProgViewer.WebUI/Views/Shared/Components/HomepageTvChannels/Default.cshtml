@model IList<TvChannelOverviewModel>
@if (Model.Count > 0)
{
    <div class="tvChannel-grid home-page-tvChannel-grid">
        <div class="title">
            <strong>@T("Homepage.TvChannels")</strong>
        </div>
        <div class="item-grid">
            @foreach (var item in Model)
            {
                <div class="item-box">
                    @await Html.PartialAsync("_TvChannelBox", item)
                </div>
            }
        </div>
    </div>
}