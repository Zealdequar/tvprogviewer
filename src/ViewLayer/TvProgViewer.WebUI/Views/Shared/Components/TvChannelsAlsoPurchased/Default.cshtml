@model IList<TvChannelOverviewModel>
@if (Model.Count > 0)
{
    <div class="also-purchased-tvchannels-grid tvchannel-grid">
        <div class="title">
            <strong>@T("TvChannels.AlsoPurchased")</strong>
        </div>
        <div class="item-grid">
            @foreach (var item in Model)
            {
                <div class="item-box">@await Html.PartialAsync("_TvChannelBox", item)</div>
            }
        </div>
    </div>
}